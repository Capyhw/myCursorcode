{
  "name": "myGGcopilot",
  "displayName": "myGGcopilot",
  "description": "a AI coder with GPT",
  "version": "0.0.1",
  "publisher": "weiyuhang",
  "icon": "resources/logo.png",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Capyhw/myCursorcode"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "GGcopilot.menulist",
        "label": "üé≤GGcopilot"
      }
    ],
    "menus": {
      "GGcopilot.menulist": [
        {
          "when": "editorTextFocus",
          "command": "GGcopilot.generation"
        },
        {
          "when": "editorTextFocus",
          "command": "GGcopilot.conversation"
        }
      ],
      "editor/context": [
        {
          "submenu": "GGcopilot.menulist",
          "when": "editorTextFocus",
          "group": "navigation"
        }
      ]
    },
    "commands": [
      {
        "command": "GGcopilot.generation",
        "title": "GGcopilotGenerationÔºàÂÖàÁî®Ëøô‰∏™Ôºâ",
        "category": "GGcopilot"
      },
      {
        "command": "GGcopilot.conversation",
        "title": "GGcopilotConversationÔºàËøô‰∏™ÊöÇÊó∂Áî®‰∏ç‰∫ÜÔºâ",
        "category": "GGcopilot"
      }
    ],
    "keybindings": [
      {
        "command": "GGcopilot.generation",
        "key": "ctrl+alt+y",
        "mac": "cmd+alt+y",
        "when": "editorTextFocus"
      },
      {
        "command": "GGcopilot.conversation",
        "key": "ctrl+alt+u",
        "mac": "cmd+alt+u",
        "when": "editorTextFocus"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "GGcopilot",
          "title": "myGGcopilot",
          "icon": "resources/logo.svg"
        }
      ]
    },
    "views": {
      "GGcopilot": [
        {
          "type": "webview",
          "id": "GGcopilot.chatView",
          "name": "GGcopilot"
        }
      ]
    },
    "configuration": {
      "title": "GGcopilotConfig",
      "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.3.4"
  }
}